ErrorDocument 404 /index.php
Options All -Indexes

<IfModule mod_rewrite.c>
  RewriteEngine on
  RewriteCond %{SERVER_PORT} 80


  
	
	RewriteRule ^(.*)$ https://www.educaauge.com/$1 [R,L]
  

	RewriteCond %{REQUEST_FILENAME}\.php -f

  RewriteRule ^([^/]+)$ $1.php [NC]
  RewriteCond %{REQUEST_FILENAME}\.html -f
  RewriteRule ^([^/]+)$ $1.html [NC]
  RewriteRule ^([^/]+)\.html$ $1.php [L]

  RewriteRule ^inicio index.php [NC]	
  RewriteRule ^iniciar-sesion iniciar-sesion_v2.php [NC]	
  RewriteRule ^registro registrate_v2.php [NC]	
	
  RewriteRule ^recuperar/([^/]+) recuperar.php?rewrite=$1 [NC]
	
  RewriteRule ^approbeseguimiento/([^/]+)/([^/]+) approbeseguimiento.php?rewrite=$1&rewrite2=$2 [NC]    
  RewriteRule ^updateseguimiento/([^/]+)/([^/]+) updateseguimiento.php?rewrite=$1&rewrite2=$2 [NC]    
	
	RewriteRule ^cesta-pago-tarjeta-directo/([^/]+) cesta-pago-tarjeta-directo.php?rewrite=$1 [NC]

	
  RewriteRule ^actualiza-tus-datos/([^/]+) actualiza-tus-datos.php?rewrite=$1 [NC]
  RewriteRule ^registrate_v2/([^/]+) registrate_v2.php?rewrite=$1 [NC]
	
  RewriteRule ^preguntas-frecuentes/([^/]+) preguntas-frecuentes.php?rewrite=$1 [NC]
	
	
  RewriteRule ^blogs/([^/]+) blog_trafico.php?rewrite2=$1 [NC]
	
  RewriteRule ^blog/([^/]+)/([^/]+) blog.php?rewrite=$1&rewrite2=$2 [NC]
  RewriteRule ^blog/([^/]+) blog.php?rewrite=$1 [NC]

  RewriteRule ^eventos/([^/]+) eventos.php?rewrite=$1 [NC]
  
  RewriteRule ^landing/([^/]+) landing.php?rewrite=$1 [NC]
  RewriteRule ^webinar/([^/]+)/([^/]+) cesta_webinar.php?rewrite=$1&rewrite2=$2 [NC]
  RewriteRule ^webinar/([^/]+) webinar.php?rewrite=$1 [NC]
  RewriteRule ^webinar2/([^/]+) webinar_forma_2.php?rewrite=$1 [NC]
  RewriteRule ^webinar3/([^/]+) webinar_forma_3.php?rewrite=$1 [NC]
  RewriteRule ^webinar4/([^/]+) webinar_forma_4.php?rewrite=$1 [NC]
  RewriteRule ^capacitate/([^/]+) webinar_forma_5_cursos.php?rewrite=$1 [NC]

	
  RewriteRule ^testimonios/([^/]+)/([^/]+) testimonios.php?rewrite=$1&rewrite2=$2 [NC]
  RewriteRule ^testimonios/([^/]+) testimonios.php?rewrite=$1 [NC]
	
	
	  RewriteRule ^clase_vivo/([^/]+) clases_en_vivos.php?rewrite=$1 [NC]

	
  RewriteRule ^tutorialprivado/([^/]+) capacitaciones.php?rewrite=$1 [NC]
  
  RewriteRule ^tutorial/([^/]+) tutoriales.php?rewrite=$1 [NC]
  RewriteRule ^gratis/([^/]+) landing_gratis.php?rewrite=$1 [NC]

  RewriteRule ^certificados/([^/]+) certificados_solicitar.php?task=$1 [NC]
  RewriteRule ^mis-libros/([^/]+) mis_libros_solicitar.php?task=$1 [NC]


  #RewriteRule ^unete/([^/]+)/([^/]+)/([^/]+)/([^/]+) trafico.php?rewrite=$1&rewrite2=$2&rewrite3=$3&rewrite4=$4 [NC]
  RewriteRule ^unete/([^/]+) trafico.php?rewrite4=$1 [NC]


  RewriteRule ^curso/([^/]+)/([^/]+)/([^/]+)/([^/]+) cursos.php?rewrite=$1&rewrite2=$2&rewrite3=$3&rewrite4=$4 [NC]
  RewriteRule ^curso/([^/]+)/([^/]+)/([^/]+) cursos.php?rewrite=$1&rewrite2=$2&rewrite3=$3 [NC]
  RewriteRule ^curso/([^/]+)/([^/]+) cursos.php?rewrite=$1&rewrite2=$2 [NC]
  RewriteRule ^curso/([^/]+) cursos.php?rewrite=$1 [NC]
	
	RewriteRule ^libro/([^/]+)/([^/]+)/([^/]+)/([^/]+) cursos.php?rewrite=$1&rewrite2=$2&rewrite3=$3&rewrite4=$4 [NC]
  RewriteRule ^libro/([^/]+)/([^/]+)/([^/]+) cursos.php?rewrite=$1&rewrite2=$2&rewrite3=$3 [NC]
  RewriteRule ^libro/([^/]+)/([^/]+) cursos.php?rewrite=$1&rewrite2=$2 [NC]
  RewriteRule ^libro/([^/]+) cursos.php?rewrite=$1 [NC]
	
	
	RewriteRule ^examen/([^/]+)/([^/]+) examenes_vender.php?rewrite=$1&rewrite2=$2 [NC]
  RewriteRule ^examen/([^/]+) examenes_vender.php?rewrite=$1 [NC]
	
	
	RewriteRule ^coautoria/([^/]+)/([^/]+)/([^/]+)/([^/]+) cursos.php?rewrite=$1&rewrite2=$2&rewrite3=$3&rewrite4=$4 [NC]
  RewriteRule ^coautoria/([^/]+)/([^/]+)/([^/]+) cursos.php?rewrite=$1&rewrite2=$2&rewrite3=$3 [NC]
  RewriteRule ^coautoria/([^/]+)/([^/]+) cursos.php?rewrite=$1&rewrite2=$2 [NC]
  RewriteRule ^coautoria/([^/]+) cursos.php?rewrite=$1 [NC]
	
	
  RewriteRule ^detalle/([^/]+)/([^/]+) curso_detalle_flotante.php?rewrite=$1&rewrite4=$2 [NC]
  RewriteRule ^trailers/([^/]+) curso_trailers_flotante.php?rewrite=$1 [NC]


  RewriteRule ^capacitacion/([^/]+) detalle_cursos.php?rewrite=$1 [NC]
  RewriteRule ^docente/([^/]+) docente.php?rewrite=$1 [NC]
  
	RewriteRule ^diplomas/([^/]+) diplomas.php?rewrite=$1 [NC]



  RewriteRule ^perfil/([^/]+)/([^/]+)/([^/]+)/([^/]+)/([^/]+) perfil.php?task=$1&task2=$2&task3=$3&task4=$4&rewrite5=$5 [NC]
  RewriteRule ^perfil/([^/]+)/([^/]+)/([^/]+)/([^/]+) perfil.php?task=$1&task2=$2&task3=$3&task4=$4 [NC]
  RewriteRule ^perfil/([^/]+)/([^/]+)/([^/]+) perfil.php?task=$1&task2=$2&task3=$3 [NC]
  RewriteRule ^perfil/([^/]+)/([^/]+) perfil.php?task=$1&task2=$2 [NC]
  RewriteRule ^perfil/([^/]+) perfil.php?task=$1 [NC]

  RewriteRule ^categoria/([^/]+) cursos.php?rewcateg=$1 [NC]
  RewriteRule ^notificaciones/([^/]+) notificaciones.php?rewrite1=$1 [NC]
	RewriteRule ^notificacion/([^/]+) comunica_contes.php?rewrite1=$1 [NC]

	RewriteRule ^consejeria/([^/]+)/([^/]+) prensa_contes.php?rewrite=$1&detalle=$2 [NC]
	RewriteRule ^consejeria/([^/]+) psicologia.php?detalle=$1 [NC]


</IfModule>
<ifModule mod_gzip.c>
  mod_gzip_on Yes
  mod_gzip_dechunk Yes
  mod_gzip_item_include file .(html?|txt|css|js|php|pl)$
  mod_gzip_item_include handler ^cgi-script$
  mod_gzip_item_include mime ^text/.*
  mod_gzip_item_include mime ^application/x-javascript.*
  mod_gzip_item_exclude mime ^image/.*
  mod_gzip_item_exclude rspheader ^Content-Encoding:.*gzip.*
</ifModule>

